{
  "name": "technical-challenge",
  "version": "1.0.0",
  "description": "technical challenge for interview process",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/index.ts",
    "start": "node ./dist/index.js",
    "eslint:fix": "eslint --fix",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "cov:html": "nyc report --reporter=html",
    "cov:check": "nyc report && nyc check-coverage --lines 95 --functions 90 --branches 90",
    "test:unit": "nyc --silent ava --serial"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaspalade/technical-challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thomaspalade/technical-challenge/issues"
  },
  "homepage": "https://github.com/thomaspalade/technical-challenge#readme",
  "dependencies": {
    "axios": "^1.2.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "stoppable": "^1.1.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/node-cache": "^4.2.5",
    "@types/sinon": "^10.0.13",
    "@types/stoppable": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "ava": "^5.1.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nock": "^13.3.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "ava": {
    "failFast": true,
    "files": [
      "!dist/**"
    ],
    "timeout": "3s",
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/test-support"
    ]
  }
}
